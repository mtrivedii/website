{
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["/api/*"]
  },
  "routes": [
    {
      "route": "/admin",
      "rewrite": "/admin.html"
    },
    {
      "route": "/scoreboard",
      "rewrite": "/scoreboard.html"
    },
    {
      "route": "/secrets/*",
      "rewrite": "/secrets/dev-notes.txt"
    }
  ],
  "mimeTypes": {
    ".json": "application/json"
  }
}
